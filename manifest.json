{
    "name": "Bookmark IT",
    "version": "1.0.0",
    "description": "Simple chrome extension to bookmark your favorite websites",
    "manifest_version": 2,
    "permissions": ["storage", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Bookmark extension",
        "default_popup": "index.html"
    }
}